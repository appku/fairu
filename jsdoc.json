{
    "plugins": [
        "plugins/markdown",
        "plugins/underscore",
        "jsdoc-plugin-intersection",
        "jsdoc-plugin-typescript",
        "jsdoc-plugin-typescript-new"
    ],
    "source": {
        "include": [
            "./",
            "./package.json"
        ],
        "exclude": [
            "./node_modules",
            "./old",
            "./assets",
            "./test",
            "./docs/",
            "./fairu.jsdoc-ignore.d.js"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)(_|.+\\.test\\.+)"
    },
    "typescript": {
        "moduleRoot": "./"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "template": "./node_modules/@appku/jsdoc-template",
        "encoding": "utf8"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "metadata": {
        "title": "@AppKu/Fairu",
        "logo": "https://avatars.githubusercontent.com/u/76977527?s=400",
        "links": [
            {
                "icon": "globe",
                "url": "https://appku.com"
            },
            {
                "icon": "github",
                "url": "https://github.com/appku/fairu"
            }
        ]
    }
}